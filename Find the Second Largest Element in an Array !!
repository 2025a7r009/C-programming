#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i = 0; i < n; i++){
        scanf("%d",&arr[i]);
    } 
    int max=arr[0];
    int smax=-10000;
    for(int i=1;i<n;i++){
        if(max<arr[i]){
            smax=max;
            max=arr[i];
                      }
    
    else if(smax<arr[i]&&arr[i]!=max) smax=arr[i];
    }
    if(smax == -10000){
    printf("No second largest element");
} else {
    printf("%d",smax);
    }
    return 0;
}
