#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int duplicate=0;
    for(int i=0;i<n;i++){
        int j;
        int count=0;
        for(j=0;j<i;j++){
            if(arr[j]==arr[i]){
                break;
            }
        }
        if(j==i){
            for(int k=i;k<n;k++){
                if(arr[k]==arr[i]){
                    count++;
                }
            }
            if(count>1){
                duplicate++;
            }
        }
    }
    printf("%d ",duplicate);
    return 0;
}
