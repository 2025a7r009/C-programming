#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    
    int m, n;
    scanf("%d %d", &m, &n);
    int arr[m][n];

    for(int i = 0; i < m; i++){
        for(int j = 0; j < n; j++){
            scanf("%d", &arr[i][j]);
        }
    }
    int sum1=0;
    for(int i = 0; i < m; i++){
        sum1=0;
        for(int j = 0; j < n; j++){
            sum1+=arr[i][j];
        }
        
    printf("%d\n",sum1);
    }
    return 0;
}
