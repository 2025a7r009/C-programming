#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int sum(int arr[],int n){
    if(n==0){
        return 0;
    }
    return arr[n-1]+sum(arr,n-1);
}

int main() {
    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int sum1=sum(arr,n);
    printf("%d",sum1);
    return 0;
}
