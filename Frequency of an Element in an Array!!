#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int m;
    scanf("%d",&m);
    int count=0;
    for(int i=0;i<n;i++){
        if(m==arr[i]){
        count++;
        }
    }printf("%d",count);
    return 0;
}
