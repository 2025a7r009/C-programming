#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int N,fact,i;
    scanf("%d",&N);
    if(N==0){
        printf("1");
    }else{
    i=N;
    fact=1;
    do{
        fact=fact*i;
        i--;
    }
    while(i>=1);
    printf("%d",fact);
    }
    return 0;
}
