#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char arr[1000];
    fgets(arr,1000,stdin);
    int count=0;
    int count1=0;
    int count2=0;
    for(int i=0;arr[i]!='\0';i++){
        if((arr[i]>='A'&&arr[i]<='Z')||(arr[i]>='a'&&arr[i]<='z')){
            count++;
        }else if(arr[i]>='0'&&arr[i]<='9'){
            count1++;
        }else count2++;
        }printf("%d %d %d",count,count1,count2);
    return 0;
}
