#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char arr[1000];
    fgets(arr,1000,stdin);
    int count=0;
    int count1=0;
    for(int i=0;arr[i]!='\0';i++){
        if(arr[i]>='A'&&arr[i]<='Z'){
            count++;
        }else if(arr[i]>='a'&&arr[i]<='z'){
            count1++;
        }
    }printf("%d %d",count,count1);
    return 0;
}
