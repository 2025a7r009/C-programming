#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() { 
    int n;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    
    for(int i=0;i<n;i++){
        int count=0;
        int j;
        for(j=0;j<i;j++){
            if (arr[i]==arr[j]){
                break;
            }
        }
        if(j==i){
            for(int k=i;k<n;k++){
                if (arr[i]==arr[k]){
                    count++;
                }
            }
            printf("%d: %d\n",arr[i],count);
        }
    }

    return 0;
}
