#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char arr[10001];
    fgets(arr,10001,stdin);
    int count=0;
    int inword=0;
    for(int i=0; arr[i]!='\0';i++){
        if((arr[i]>='A' && arr[i]<='Z')||(arr[i]>='a'&&arr[i]<='z')){
            if(inword==0){
                count++;
                inword=1;
            }
        }else{
            inword=0;
        }
    }printf("%d",count);
    return 0;
}
