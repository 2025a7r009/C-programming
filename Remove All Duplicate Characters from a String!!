#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char s[1000];
    fgets(s, 1000, stdin);

    int seen[256] = {0};

    for (int i = 0; s[i] != '\0'; i++) {
        if (seen[s[i]] == 0) {
            printf("%c", s[i]);
            seen[s[i]] = 1;
        }
    }
    return 0;
}
